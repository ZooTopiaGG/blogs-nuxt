{
  "apps": {
    "name": "55lover-front",
    "script": "cross-env MODE=pro nuxt build && cross-env MODE=pro nuxt start",
    "cwd": "./",
    "args": "",
    "interpreter": "",
    "interpreter_args": "",
    "watch": true,
    "ignore_watch": ["node_modules", "logs"],
    "instances": 1,
    "max_memory_restart": 8,
    "error_file": "./logs/error.log",
    "out_file": "./logs/out.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "min_uptime": "60s",
    "max_restarts": 30,
    "autorestart": true,
    "cron_restart": "",
    "env": {
      "NODE_ENV": "production",
      "REMOTE_ADDR": "https://www.55lover.com/"
    },
    "env_dev": {
      "NODE_ENV": "development",
      "REMOTE_ADDR": ""
    },
    "env_test": {
      "NODE_ENV": "test",
      "REMOTE_ADDR": ""
    }
  }
}
